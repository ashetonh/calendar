#!/bin/bash
if[ $1 -eq 0 ]
then
    curl -X PUT -H "Content-Type: application/json" -d @../src/main/resources/static/testEvent.json localhost:8080/api/1/update-meeting/2
fi
if[ $1 -gt 0 ]
then
    curl -X PUT -H "Content-Type: application/json" -d @../src/main/resources/static/testEvent.json localhost:8080/api/1/update-meeting/$1
fi